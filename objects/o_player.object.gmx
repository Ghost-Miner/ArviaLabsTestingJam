            <kind>1</kind>
            <string>///controls and gravity 

//Get player's input
key_right = keyboard_check(vk_right) or keyboard_check(ord("D")); // (keyboard_check(global.KeyRight));
key_left = keyboard_check(vk_left) or keyboard_check(ord("A")); //(keyboard_check(global.KeyLeft));
key_jump = keyboard_check(vk_up) or keyboard_check(ord("W")); //(keyboard_check_pressed(global.KeyJump));

//React to inputs
move = key_left - key_right;
hsp = move * movespeed;
if (vsp &lt; 10) vsp += grav;
    
if (place_meeting(x,y+1,o_wall))
{
    jumps = jumpsmax;
}

if ((key_jump) and (jumps &gt; 0))
{
    jumps -= 1;
    vsp = -jumpspeed;
    global.Jumps += 1;
}

if Move = true {

//Horizontal collision
if (place_meeting(x+hsp,y,o_wall))
{
    while(!place_meeting(x+sign(hsp),y,o_wall))
    {
        x += sign(hsp);
    }
    hsp = 0;
}
x += hsp; 
}

//vertical collision
if (place_meeting(x,y+vsp,o_wall))
{
    while(!place_meeting(x,y+sign(vsp),o_wall))
    {
        y += sign(vsp);
    }
    vsp = 0;
}
y += vsp;

</string>
